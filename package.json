    "react-event-listener": "^0.4.0",
    "react-fuzzy": "^0.3.3",
    "react-fuzzy-filter": "^2.3.0",
    "react-icons": "^2.2.1",
    "react-komposer": "^1.13.1",
    "react-modal": "^1.4.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-bootstrap": "^0.23.1",
