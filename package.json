    "redux": "^3.6.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "reflexbox": "^2.2.2",
    "screenfull": "^3.0.2",
    "simpl-schema": "0.0.3",
    "superagent": "^2.1.0",
    "typescript": "^2.0.7",
    "typings": "^1.3.2",
    "underscore": "^1.8.3",
    "validatorjs": "^3.8.0",
    "victory": "^0.13.0",
    "xlsx": "^0.8.0"
  },
  "engines": {
    "node": "4.4.7"
  }
}
