{
  "version": "0.2.0",
  "configurations": [{
    "name": "Launch",
    "type": "node",
    "request": "launch",
    "program": "${workspaceRoot}/server/main.js",
    "stopOnEntry": false,
    "args": [],
    "cwd": "${workspaceRoot}/server/",
    "preLaunchTask": null,
    "runtimeExecutable": null,
    "runtimeArgs": [
      "--nolazy"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "externalConsole": false,
    "sourceMaps": false,
    "outDir": null
  }, {
    "name": "Attach",
    "type": "node",
    "request": "attach",
    "port": 5858,
    "address": "localhost",
    "restart": false,
    "sourceMaps": true,
    "outDir": "${workspaceRoot}/.meteor/local/build/programs/server",
    "localRoot": "${workspaceRoot}",
    "remoteRoot": null,
    // Uncomment this to run easily alongside another running instance of Chrome
    "userDataDir": "${workspaceRoot}/.vscode/chrome",

    // Uncomment this to get diagnostic logs in the console
    "diagnosticLogging": true
  }]
}
